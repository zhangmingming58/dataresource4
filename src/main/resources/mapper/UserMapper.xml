<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zhangkai.dao.UserMapper">
	<insert id="insertUser">
		insert into t_user
		(name,gender,mobile,email,birthday) values
		(#{name},#{gender},#{mobile},#{email},#{birthday})
	</insert>

</mapper>